# Uncomment the next line to define a global platform for your project
platform :ios, '10.1'

target 'BNForm' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for BNForm
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'FBSDKCoreKit'
  pod 'FBSDKShareKit'
  pod 'FBSDKLoginKit'
  pod 'AlamofireRSSParser'
  pod 'SwiftyJSON'

  target 'BNFormTests' do
    use_frameworks!
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase/Core'
    pod 'Firebase/Auth'
    pod 'AlamofireRSSParser'
    pod 'SwiftyJSON'
  end

  target 'BNFormUITests' do
    inherit! :search_paths
    # Pods for testing
  end
  

end
